    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@visx/visx": "^2.18.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.6",
    "balloon-css": "^1.2.0",
    "d3": "^7.8.2",
    "dayjs": "^1.11.7",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "jquery": "^3.6.3",
    "next": "13.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-transition-group": "^4.4.5",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/jquery": "^3.5.16",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "concurrently": "^7.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.1",
    "tailwindcss": "^3.2.4"
